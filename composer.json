{
  "name": "generoi/wp-image-resizer",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A plugin which provides dynamic image sizes through a CDN",
  "homepage": "https://github.com/generoi/wp-image-resizer",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/generoi/wp-image-resizer/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.minasithil.genero.fi/"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "oscarotero/env": "^2.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "consolidation/robo": "^3.0",
    "generoi/robo-genero": "dev-master",
    "phpstan/phpstan": "^1.10",
    "szepeviktor/phpstan-wordpress": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "GeneroWP\\ImageResizer\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "phpcs",
      "phpstan"
    ]
  },
  "archive" : {
    "exclude": [
      ".gitignore"
    ]
  }
}
